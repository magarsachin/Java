
// Program to check if given number is a palindrome
class Palindrome2 {
    public boolean isPalindrome(int x) {
        int m = x;
        int p = 0;
        if (x<0)
        return false;
     else{   
    while(x>0){
        p = p*10 + (x%10);
        x = x/10;
    }
    }
    if (m==p)
    return true;
    else
    return false;
}
}


public class Palindrome {
	
	public static void main(String[] args) {
		
		Palindrome2 obj = new Palindrome2();
		int x = 121;
		System.out.println(obj.isPalindrome(x));
	}

}
